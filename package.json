{
  "name": "openclaw-webchat-client",
  "private": false,
  "version": "0.1.0",
  "description": "OpenClaw Web Chat Client（Monorepo）",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:api": "npm -w apps/api run dev",
    "dev:web": "npm -w apps/web run dev",
    "build": "npm -w apps/api run build && npm -w apps/web run build",
    "build:api": "npm -w apps/api run build",
    "build:web": "npm -w apps/web run build",
    "start:api": "npm -w apps/api run start"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
